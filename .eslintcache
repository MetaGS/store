};\r\n    setCreditCard(newValue);\r\n    return newValue;\r\n  };\r\n\r\n  return [\r\n    creditCard,\r\n    {\r\n      handleExpiration,\r\n      handleCvv,\r\n      handleCreditCardNumber,\r\n    },\r\n  ];\r\n};\r\n","C:\\Users\\Munar\\Desktop\\Projects\\reactCodeLearner\\store\\src\\components\\CautionP.js",["798"],"import React from \"react\";\r\nimport PropTypes from \"prop-types\";\r\n\r\nimport \"./CautionP.css\";\r\nconst CautionP = ({ children, text, className = \"\", ...props }) => {\r\n  return (\r\n    <p {...props} className={`__caution-p ${className}`}>\r\n      {(text, children)}\r\n    </p>\r\n  );\r\n};\r\n\r\nCautionP.propTypes = {};\r\n\r\nexport default CautionP;\r\n","C:\\Users\\Munar\\Desktop\\Projects\\reactCodeLearner\\store\\src\\firebase\\db\\submitOrder.js",[],"C:\\Users\\Munar\\Desktop\\Projects\\reactCodeLearner\\store\\src\\components\\TitleAbhayaWithoutAnimations.js",["799"],"C:\\Users\\Munar\\Desktop\\Projects\\reactCodeLearner\\store\\src\\components\\404.js",["800"],{"ruleId":"801","severity":1,"message":"802","line":24,"column":5,"nodeType":"803","messageId":"804","endLine":24,"endColumn":8},{"ruleId":"805","replacedBy":"806"},{"ruleId":"807","replacedBy":"808"},{"ruleId":"801","severity":1,"message":"809","line":1,"column":25,"nodeType":"803","messageId":"804","endLine":1,"endColumn":38},{"ruleId":"801","severity":1,"message":"810","line":1,"column":20,"nodeType":"803","messageId":"804","endLine":1,"endColumn":29},{"ruleId":"801","severity":1,"message":"811","line":2,"column":10,"nodeType":"803","messageId":"804","endLine":2,"endColumn":18},{"ruleId":"801","severity":1,"message":"812","line":6,"column":8,"nodeType":"803","messageId":"804","endLine":6,"endColumn":16},{"ruleId":"801","severity":1,"message":"813","line":15,"column":8,"nodeType":"803","messageId":"804","endLine":15,"endColumn":15},{"ruleId":"801","severity":1,"message":"814","line":16,"column":8,"nodeType":"803","messageId":"804","endLine":16,"endColumn":20},{"rule